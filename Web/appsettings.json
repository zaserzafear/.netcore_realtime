{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSetting": {
    "Issuer": "localhost",
    "Audience": "localhost",
    "SigningKey": "secret",
    "RequestQuery": "access_key",
    "JwtTokenValidateUrl": "http://api/api/Authentication/ValidateToken",
    "AuthKey": "AuthToken"
  },
  "Session": {
    "IdleTimeoutMinutes": 30
  },
  "ServiceUrlSetting": {
    "AuthApi": "http://api/api/Authentication",
    "ChatApi": "http://api/api/Chat",
    "ChatHub": "http://localhost:5000/Hub"
  }
}
